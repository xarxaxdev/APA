#./bin/bash

echo "Hello world"

echo "/opt/pccts/bin/antlr -gt  example1.g"
/opt/pccts/bin/antlr -gt  example1.g
echo "/opt/pccts/bin/dlg -ci parser.dlg scan.c"
/opt/pccts/bin/dlg -ci parser.dlg scan.c

echo "g++ -o lego lego.c scan.c err.c -I/home/soft/PCCTS_v1.33/include/ -Wno-write-strings"
g++ -o example1 example1.c scan.c err.c -I/home/soft/PCCTS_v1.33/include/ -Wno-write-strings
echo "executing program"
./example1 < jp
